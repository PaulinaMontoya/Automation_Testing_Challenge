{
  "name": "tenku_automation_testing",
  "version": "1.0.0",
  "description": "e2e Automation Testing",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ui-test": "cypress open",
    "headless-chrome": "cypress run --browser chrome --spec cypress/e2e",
    "headless-firefox": "cypress run --browser firefox --spec cypress/e2e",
    "headless-electron": "cypress run --browser electron --spec cypress/e2e",
    "headless-edge": "cypress run --browser edge --spec cypress/e2e",
    "api-test": "newman run 'api/Kiosko_Backend_Challenge.postman_collection.json' -e 'api/envVariables.postman_environment.json'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulinaMontoya/Tenku_Automation_Testing.git"
  },
  "keywords": [
    "e2e-automation-testing",
    "cypress"
  ],
  "author": "Paulina Montoya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaulinaMontoya/Tenku_Automation_Testing/issues"
  },
  "homepage": "https://github.com/PaulinaMontoya/Tenku_Automation_Testing#readme",
  "devDependencies": {
    "cypress": "^13.13.2",
    "cypress-wait-until": "^3.0.2",
    "cypress-xpath": "^2.0.1",
    "newman": "^6.2.0"
  }
}
